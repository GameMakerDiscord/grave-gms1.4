            <string>/// Create the enemies
// Count the enemies
var enemy_count = instance_number(o_enemy);

// Create new enemies
if (instance_exists(o_skeleton) &amp;&amp; enemy_count &lt; o_skeleton_stats.kills/4) {

    // Exit if there are more than 5
    if (enemy_count &gt; 5) exit;
    
    // Select an enemy
    var enemy = choose(o_crow, o_crow, o_knight);
    if (o_skeleton_stats.kills &gt; 20 &amp;&amp; !instance_exists(o_boss)) {
        enemy = choose(o_crow, o_crow, o_knight, o_knight, o_boss);
    }
    
    // Select a location
    var xx = random_range(220, room_width-220);
    while (point_distance(xx, 0, o_skeleton.x, 0) &lt; 220) {
        xx = random_range(220, room_width-220);
    }
    
    // Create the enemy
    var yy = o_skeleton.y;
    instance_create(xx, yy, enemy);
}


